# 2GIS to Traccar Bridge Environment Configuration
# Copy this file to .env and update the values

# REQUIRED: 2GIS WebSocket Configuration
# Get your token from 2GIS app and replace YOUR_TOKEN_HERE
TWOGIS_WS_URL=wss://zond.api.2gis.ru/api/1.1/user/ws?appVersion=6.31.0&channels=markers,sharing,routes&token=YOUR_TOKEN_HERE

# REQUIRED: Traccar Server Configuration
# Your Traccar server URL with OsmAnd port (usually 5055)
TRACCAR_BASE_URL=http://your-traccar-server:5055

# Optional: Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=2gis2traccar.log

# Optional: Reconnection settings
RECONNECT_DELAY=30
MAX_RECONNECT_ATTEMPTS=10
